using lscript.io

class Main

	function static interp void main(objectarray String args)
		if args.length[0] == true
			ret
		end

		object FileOutputStream fout
		seto fout new FileOutputStream(LString;) "test.txt"
		dynamic_call fout.print(LString;) "Hello World"
		dynamic_call fout.close()

		object FileInputStream fin
		object String line
		seto fin new FileInputStream(LString;) "test.txt"
		dynamic_call fin.readLine()
		setr line

		dynamic_call System.stdout.println(LString;) line

		ret
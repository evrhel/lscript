package lscript.lang

class Long

	function static interp object toString(long value)
		object StringBuilder builder
		seto builder new StringBuilder <init>()
		dynamic_call builder.append(Q) value
		dynamic_call builder.toString()
		retr

	global dynamic varying long value

	constructor()
		ret

	function dynamic interp void <init>(long value)
		setv this.value value
		ret

	function dynamic interp object toString()
		static_call toString(Q) this.value
		retr

package lscript.lang

class Object extends null

	constructor()
		ret

	function Class getClass()
		static_call Class.classOf(LObject;) this
		retr

	function void threadEntry()
		ret

	function String toString()
		static_call Long.toString(Q) this
		retr

	function bool equals(Object other)
		if this == other
			retb byte[1]
		else
			retb byte[0]
		end

	function uint hashCode()
		retv this

	function void finalize()
		ret




class Long

	function static interp toString(long value)
		object builder
		seto builder new StringBuilder <init>()
		dynamic_call builder.append(Q) value
		dynamic_call builder.toString()
		retr

	global dynamic varying long value

	function dynamic interp <init>()
		ret

	function dynamic interp <init>(long value)
		setv this.value value
		ret

	function dynamic interp toString()
		static_call toString(Q) this.value
		retr
